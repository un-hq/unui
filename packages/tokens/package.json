{
  "name": "@unhq/tokens",
  "version": "9.0.0",
  "private": false,
  "license": "MIT",
  "homepage": "https://<your-docs-site>/<pkg>",
  "bugs": {
    "url": "https://github.com/un-hq/unui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/un-hq/unui.git",
    "directory": "packages/tokens"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css.d.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "styles.css",
    "styles.css.d.ts",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "./dist/styles.css",
    "./styles.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean && node -e \"const fs=require('fs'),p='dist';fs.mkdirSync(p,{recursive:true});fs.copyFileSync('src/styles.css',`${p}/styles.css`);fs.writeFileSync(`${p}/styles.css.d.ts`,`declare const css: string;\\nexport default css;\\n`);fs.copyFileSync('src/styles.css','styles.css');fs.writeFileSync('styles.css.d.ts',`declare const css: string;\\nexport default css;\\n`);\"",
    "typecheck": "tsc --noEmit",
    "lint": "echo ok",
    "prepack": "pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
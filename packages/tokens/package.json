{
  "name": "@unhq/tokens",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "6.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/styles.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean && node -e \"require('fs').mkdirSync('dist',{recursive:true});require('fs').copyFileSync('src/styles.css','dist/styles.css')\"",
    "typecheck": "tsc --noEmit",
    "lint": "echo ok"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
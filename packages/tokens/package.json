{
  "name": "@unhq/tokens",
  "private": false,

  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/un-hq/unui.git", "directory": "packages/tokens" },
  "bugs": { "url": "https://github.com/un-hq/unui/issues" },
  "homepage": "https://<your-docs-site>/<pkg>",
  "publishConfig": { "access": "public" },
  "files": ["dist", "package.json", "README.md", "LICENSE"],
  "version": "6.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "dist/styles.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean && node -e \"require('fs').mkdirSync('dist',{recursive:true});require('fs').copyFileSync('src/styles.css','dist/styles.css')\"",
    "typecheck": "tsc --noEmit",
    "lint": "echo ok"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
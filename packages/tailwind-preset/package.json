{
  "name": "@unhq/tailwind-preset",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "6.0.0",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "tailwindcss": "^3.4.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean",
    "typecheck": "tsc --noEmit",
    "lint": "echo ok"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}

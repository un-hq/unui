{
    "name": "@unhq/blocks",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "version": "4.0.0",
    "type": "module",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "peerDependencies": {
        "@unhq/ui": ">=5.0.0",
        "react": ">=18",
        "react-dom": ">=18"
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm --clean",
        "typecheck": "tsc --noEmit",
        "lint": "echo ok",
        "size": "pnpm run build && size-limit"
    },
    "devDependencies": {
        "@size-limit/esbuild": "^11.2.0",
        "@size-limit/file": "^11.2.0",
        "@types/react": "^18.2.66",
        "@unhq/ui": "workspace:*",
        "esbuild": "^0.25.10",
        "size-limit": "^11.2.0",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}